@model IEnumerable<Task2.Models.UserModel>

@foreach (var item in Model)
{
    <tr>
        <td>@item.Name</td>
        <td>@item.Email</td>
        <td>@item.DateOfBirth</td>
        <td>@item.Address</td>
        <td>@item.City</td>
        <td>
            <button class="btn btn-sm btn-outline-primary" onclick="loadEditForm('@item.Id')">Edit</button>
            <button class="btn btn-sm btn-outline-success" onclick="loadDetails('@item.Id')">Details</button>
            <button class="btn btn-sm btn-outline-danger" onclick="loadDeleteForm('@item.Id')">Delete</button>
        </td>
    </tr>
}